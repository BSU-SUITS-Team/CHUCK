{
    "extends": "./.svelte-kit/tsconfig.json",
    "include": ["src/**/*.d.ts", "src/**/*.ts", "src/**/*.svelte"],
    "exclude": ["node_modules/*", "__sapper__/*", "public/*"],
    "compilerOptions": {
      "moduleResolution": "node",
      "baseUrl": ".",
      "paths": {
        "$app/*": ["./node_modules/@sveltejs/kit/types/app/*"],
        "$lib":["./src/lib"],
        "$lib/*":["./src/lib/*"]
      }
    }
  }
  